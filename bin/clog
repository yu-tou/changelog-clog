#!/usr/bin/env node


var program = require('commander');

program
  .version('0.1.0')
  .command('changelog','生成changelog.md文件在当前目录下')

program
  .version('0.1.0')
  .command('dingding [others...]','发送最后一次发布后的记录到钉钉群')

program
  .version('0.1.0')
  .command('changelog-standard', '生成 changelog')

    // c.sendLastReleaseMessage(args.userName,args.sdkName,args.pushURL)

program.parse(process.argv);
